{
    "version":"0.1.0",
    "resource":"API Glass Bank",
    "endpoints":[
        {
            "endpoint":"api/agency",
            "httpMethod":"GET",
            "description":"all agency for the API Docs.",
            "shortDecription":"agency",
            "url":"/api/agency",
            "output":"{ 'agency':'agency'},"
        },
        {
            "endpoint":"api/agency/:id",
            "httpMethod":"GET",
            "description":"get agency for the API Docs.",
            "shortDecription":"get agency",
            "url":"/api/agency/:id",
            "parameters":[
                          {
                          	 "parameter":"id",
                               "value":"(number)",
                               "description": "ID of agency"            
                          }                 
 
            ],            
            "output":"{ 'agency':'agency'},"
        },          
        {
            "endpoint":"api/agency",
            "httpMethod":"POST",
            "description":"create agency for the API Docs.",
            "shortDecription":"agency",
            "url":"/api/agency",
            "parameters":[
                          {
                          	 "parameter":"name",
                               "value":"(String)",
                               "description": "name of agency"            
                          },
                          {
                           	 "parameter":"phone",
                                "value":"(number)",
                                "description": "phone of agency"            
                           }               
 
            ],
            "output":"{ 'agency' : 'agency'},"        	
        },
        {
            "endpoint":"api/agency/:id",
            "httpMethod":"DELETE",
            "description":"delete agency for the API Docs.",
            "shortDecription":"del agency",
            "url":"/api/creditcard/:id",
            "parameters":[
                          {
                          	 "parameter":"id",
                               "value":"(number)",
                               "description": "ID of agency"            
                          }                 
 
            ],            
            "output":"{ 'status':'ok'},"
        }       
    ]
} 
