            ],            
            "output":"{ 'photos':'photos'},"
        },      
        {
            "endpoint":"person",
            "httpMethod":"POST",
            "description":"create person for the API Docs.",
            "shortDecription":"person",
            "url":"/api/person",
            "parameters":[
                          {
                          	 "parameter":"userId",
                               "value":"(number)",
                               "description": "ID of user"            
                          },
                          {
                           	 "parameter":"last_name",
                                "value":"(String)",
                                "description": "last_name of person"            
                           },
                           {
                            	 "parameter":"first_name",
                                 "value":"(String)",
                                 "description": "first_name of person"            
                            },
                            {
                            	 "parameter":"mail",
                                 "value":"(String)",
                                 "description": "mail of person"            
                            },                            
                            {
                           	 "parameter":"address",
                                "value":"(String)",
                                "description": "address of person"            
                           },  
                           {
                          	 "parameter":"postcode",
                               "value":"(String)",
                               "description": "postcode of person"            
                          },  
                          {
                         	 "parameter":"city",
                              "value":"(String)",
                              "description": "mail of person"            
                         },  
                         {
                        	 "parameter":"city",
                             "value":"(String)",
                             "description": "city of person"            
                        },  
                        {
                       	 "parameter":"country",
                            "value":"(String)",
                            "description": "country of person"            
                       },
                       {
                      	 "parameter":"phone",
                           "value":"(number)",
                           "description": "phone of person"            
                      }                       
 
            ],
            "output":"{ 'person' : 'person'},"        	
        },
        {
            "endpoint":"person",
            "httpMethod":"DELETE",
            "description":"delete person for the API Docs.",
            "shortDecription":"del person",
            "url":"/api/person/:id",
            "parameters":[
                          {
                          	 "parameter":"id",
                               "value":"(number)",
                               "description": "ID of person"            
                          }                 
 
            ],            
            "output":"{ 'status':'ok'},"
        }       
    ]
} 
