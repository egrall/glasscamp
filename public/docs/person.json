{
    "version":"0.1.0",
    "resource":"API Glass Bank",
    "endpoints":[
        {
            "endpoint":"person",
            "httpMethod":"GET",
            "description":"All person for the API Docs.",
            "shortDecription":"All person",
            "url":"/api/person",
            "output":"{ 'person':'person'},"
        },
        {
            "endpoint":"person/:id",
            "httpMethod":"GET",
            "description":"get person for the API Docs.",
            "shortDecription":"get person",
            "url":"/api/person/:id",
            "parameters":[
                          {
                          	 "parameter":"id",
                               "value":"(number)",
                               "description": "ID of person"            
                          }                 
 
            ],            
            "output":"{ 'person':'person'},"
        },
        {
            "endpoint":"person/adresses/:userId",
            "httpMethod":"GET",
            "description":"get adresses of person for the API Docs.",
            "shortDecription":"get adresses",
            "url":"/person/adresses/:userId",
            "parameters":[
                          {
                          	 "parameter":"userId",
                               "value":"(number)",
                               "description": "ID of userId"            
                          }                 
 
            ],            
            "output":"{ 'adresses':'adresses'},"
        },
        {
            "endpoint":"person/phones/:userId",
            "httpMethod":"GET",
            "description":"get phones of person for the API Docs.",
            "shortDecription":"get phones",
            "url":"/person/phones/:userId",
            "parameters":[
                          {
                          	 "parameter":"userId",
                               "value":"(number)",
                               "description": "ID of userId"            
                          }                 
 
            ],            
            "output":"{ 'phones':'phones'},"
        },
        {
            "endpoint":"person/photos/:userId",
            "httpMethod":"GET",
            "description":"get photos of person for the API Docs.",
            "shortDecription":"get photos",
            "url":"/person/photos/:userId",
            "parameters":[
                          {
                          	 "parameter":"userId",
                               "value":"(number)",
                               "description": "ID of userId"            
